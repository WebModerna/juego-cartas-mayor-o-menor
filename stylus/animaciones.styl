.garcando
  animation-name garcando

@keyframes garcando
  0%
    width 0px
  
  100%
    width 200px


.fadeOut
  animation-name fadeOut

@keyframes fadeOut
  0%
    opacity 1
    visibility visible
  95%
    opacity 1
    visibility visible
  100%
    opacity 0
    visibility hidden
  

.zoomOut
  animation-name zoomOut
  animation-timing-function: ease 
  animation-iteration-count: 1

@keyframes zoomOut
  0%
    opacity 1
    transform scale(1)
    visibility visible
  
  100%
    opacity 0
    transform scale(0)
    visibility hidden
    display: none !important